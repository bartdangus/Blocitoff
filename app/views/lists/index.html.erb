<h1> Lists 6</h1>

<div class="row">
  <div class="col-md-8">
    <% @lists.each do |list| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to list.title, list %>
          </h4>
        </div>
      </div>
      <% end %>
    </div>
  </div>



<div class="col-md-4">
  <% if policy(List.new).create? %>
    <%= link_to "New List", new_list_path, class: 'btn btn-success' %>
    <% end %>
  </div>